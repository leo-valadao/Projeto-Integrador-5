{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Chrome - Debug",
      "type": "chrome",
      "request": "launch",
      "url": "http://127.0.0.1:4200/",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Firefox - Debug",
      "type": "firefox",
      "request": "launch",
      "url": "http://127.0.0.1:4200/",
      "webRoot": "${workspaceFolder}",
      "reAttach": true
    }
  ]
}
